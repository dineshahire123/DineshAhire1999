import java.util.*;


class Main{
    
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();
         int b = sc.nextInt();
          int c = sc.nextInt();
           int d = sc.nextInt();
           
           check(a,b,c,d);
    }
    
    
    public static void check(int a, int b, int c, int d)
    {
        if(a>b && c>d)
        {
            System.out.println(true);
        }
        else
        {
            System.out.println(false);
        }
        
    }
}
