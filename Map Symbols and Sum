function mapSymbolsSum(N, str) {
  //write code here
  
  var char = ["!","@","#","$","%","^","&","*"];
  var num =  [1,2,3,4,5,6,7,8];
  var sum = 0;
  for(i=0; i<N; i++)
  {
      for(j=0; j<num.length; j++)
      {
          if(str[i]==char[j])
          {
              sum = sum + num[j];
          }
      }
  }
  console.log(sum);
}
