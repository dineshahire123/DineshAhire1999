//Enter code here
import java.util.*;


class Main{
    
    
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        String str = "abcdefghijklmnopqrstuvwxyz";
        int j = 0;
        for(int i=n; i<n + str.length(); i++)
        {
            System.out.println(str.charAt(j++) + "-" + i);
            
        }
    }
}
