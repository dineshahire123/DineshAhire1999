function binaryMatrix(N,M, matrix) {
  //write code here
var arr = [1,0]
  for(i=0; i<N; i++)
  {
        var bag = "";
      for(j=0; j<M; j++)
      {
          if(matrix[i][j]==0)
          {
          bag = bag  + arr[0] + " ";
          }
          if(matrix[i][j]==1)
          {
              bag = bag + arr[1] + " ";
          }
      }
      console.log(bag);
  }
  
}
