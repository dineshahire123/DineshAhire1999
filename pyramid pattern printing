 class PatternPrinting {
    public static void main(String[] args) {
        int rows = 5;

        // Pattern 3: Pyramid
        System.out.println("Pattern 3: Pyramid");
        for (int i = 1; i <= rows; i++) {
            for (int j = rows; j > i; j--) {
                System.out.print(" ");
            }
            for (int k = 1; k <= 2 * i - 1; k++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
